    "stylelint": "8.0.0",
    "stylelint-config-primer": "2.0.1",
    "stylelint-config-standard": "17.0.0",
    "stylelint-config-styled-components-processor": "0.1.1",
    "stylelint-order": "0.6.0",
    "stylelint-processor-styled-components": "0.4.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --no-ignore --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar public/assets/global/sass/ -o public/assets/global/static/ --use autoprefixer && npm run css-minify",
    "watch-css": "npm run build-css && node-sass-chokidar public/assets/global/sass/ -o public/assets/global/static/ --source-map true --watch --recursive  --source-map-contents --source-map-root",
    "css-minify": "csso public/assets/global/static/app.css --output public/assets/global/static/app.min.css",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" . && stylelint \"./src/**/*.js\"",
    "fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
    "relay": "node ./setup --download-schema && relay-compiler --src ./src --schema ./src/schema.graphql",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "deploy": "node ./setup && react-scripts build && gh-pages -d build",
    "start-js": "react-scripts start",
    "start": "node ./setup && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build-stage": "node ./setup && npm-run-all build-css build-js && node ./render",
    "build-prod": "node ./setup && npm-run-all build-css build-js && node ./render",
    "build": "node ./setup && npm-run-all build-css build-js && node ./render",
    "build-dev": "node ./setup && npm-run-all build-css build-js && node ./render"
  }
}
